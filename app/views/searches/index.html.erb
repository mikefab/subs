<h1>Listing searches</h1>

<table>
  <tr>
    <th>Search</th>
    <th>User</th>
    <th>Lang</th>
    <th>Category</th>
    <th>Page</th>
    <th>Option</th>
	<th>Created</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @searches.each do |search| %>
  <tr>
    <td><%= search.search %></td>
    <td><%= search.user_id %></td>
    <td><%= search.lang %></td>
    <td><%= search.category %></td>
    <td><%= search.page %></td>
    <td><%= search.option %></td>
    <td><%= search.created_at %></td>
    <td><%= link_to 'Show', search %></td>
    <td><%= link_to 'Edit', edit_search_path(search) %></td>
    <td><%= link_to 'Destroy', search, :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Search', new_search_path %>
